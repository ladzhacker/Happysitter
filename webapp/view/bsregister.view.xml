<mvc:View
    controllerName="com.applexus.happysitter.controller.bsregister"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
>
    <Toolbar id="_IDGenToolbar1">
        <Button
            id="_IDGenButton1"
            type="Back"
            press="onPress"
        />
        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
        <Title
            id="_IDGenTitle1"
            text="Babysitter Registration"
            level="H2"
        />
        <ToolbarSpacer id="_IDGenToolbarSpacer2" />
    </Toolbar>
    <f:SimpleForm
        id="SimpleFormChange354"
        editable="true"
        layout="ResponsiveGridLayout"
        title="Basic Details"
        labelSpanXL="3"
        labelSpanL="3"
        labelSpanM="3"
        labelSpanS="12"
        adjustLabelSpan="false"
        emptySpanXL="4"
        emptySpanL="4"
        emptySpanM="4"
        emptySpanS="0"
        columnsXL="1"
        columnsL="1"
        columnsM="1"
        singleContainerFullSize="false"
    >
        <f:content>
            <Label
                id="_IDGenLabel1"
                text="Name"
                required="true"
            />
            <Input
                id="name"
                value="{}"
                type="Text"
                maxLength="20"
                liveChange="onLiveChange"
            />

            <Label
                id="_IDGenfGLabel1"
                text="e-mail"
                required="true"
            />
            <Input
                id="ndame"
                value="{}"
                type="Email"
                liveChange="onLiveChange"
                
            />
            <Label
                id="_IDGenGLabel1"
                text="Password"
                required="true"
            />
            <Input
                id="ndagme"
                value="{}"
                type="Password"
                maxLength="8"
                liveChange="onLiveChange"
                placeholder="Password must be 8 characters"
            />
            <Label
                id="_IDGtenfGLabel1"
                text="Phonenumber"
                required="true"
            />
            <Input
                id="ndamhe"
                value="{}"
                type="Number"
                maxLength="10"
            />
            <Label
                id="_IDGenLabel2"
                text="Gender"
                required="true"
            />
            <RadioButtonGroup
                id="r6bg3"
                columns="5"
                valueState="Error"
                class="sapUiMediumMarginBottom"
            >
                <RadioButton
                    id="RBY3-1"
                    text="M"
                />
                <RadioButton
                    id="RBG3-2"
                    text="F"
                />
            </RadioButtonGroup>
            <Label
                id="_IFDGenLabel2"
                text="DOB"
                required="true"
            />
            <DatePicker
                id="DP2"
                change="handleChange"
                class="sapUiSmallMarginBottom"
            />

            <Label
                id="_IFDGenLabel92"
                text="Preferred Location"
                required="true"
            />

            <Select
                id="p1"
                selectedKey="{PreferredLocation}"
            >
                <core:Item
                    id="l1"
                    key="KZM"
                    text="Kazhakootam"
                />

                <core:Item
                    id="l2"
                    key="KLT"
                    text="Kulathoor"
                />
                
                <core:Item
                    id="l4"
                    key="ULO"
                    text="Ulloor"
                />
            </Select>
            <Label
                id="s"
                text="Preferred Shift"
                required="true"
            />
            <Select
                id="s1"
                selectedKey="{PreferredShift}"
            >
                <core:Item
                    id="it1"
                    key="M"
                    text="Morning"
                />

                <core:Item
                    id="it2"
                    key="N"
                    text="Night"
                />
                <core:Item
                    id="itf2"
                    key="F"
                    text="Fulltime"
                />
            </Select>
              <ComboBox

              id="cb5"

              items="{/babysitterSet}"

             >

              <core:Item

             id="item5"

            text="{PreferredLocation}"

                  />

             </ComboBox>  
              
            <Label
                id="_IFDGenLabel41"
                text="Certification ID"
            />
            <Input
                id="cid"
                value=""
                type="Number"
            />
            <Label
                id="_IFDGenLabel451"
                text="Aadhaar No."
                required="true"
            />
            <Input
                id="aid"
                value=""
                type="Number"
                maxLength="12"
            />

            <Label
                id="_IFDGenLabel4951"
                text="Multiple Kid Option"
                required="true"
            />

            <RadioButtonGroup
                id="rbg3"
                columns="5"
                valueState="Error"
                class="sapUiMediumMarginBottom"
            >
                <RadioButton
                    id="RB3-1"
                    text="Y"
                />
                <RadioButton
                    id="RB3-2"
                    text="N"
                />
            </RadioButtonGroup>
            <Label
                id="_IHFDGenInput3701"
                text=""
            />
            <Button
                id="t690"
                text="Submit"
                type="Accept"
                width="45%"
                press="oncreate"
            />
        </f:content>
    </f:SimpleForm>
</mvc:View>
