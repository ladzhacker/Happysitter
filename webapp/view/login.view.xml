<mvc:View
    xmlns:vbm="sap.ui.vbm"
    controllerName="com.applexus.happysitter.controller.login"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <VBox
            id="_IDGenVBox1"
            class="sapUiSmallMargin"
        >
            <f:SimpleForm
                id="SimpleFormChange354"
                editable="true"
                layout="ResponsiveGridLayout"
                title="Happysitter Login"
                labelSpanXL="3"
                labelSpanL="3"
                labelSpanM="3"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="4"
                emptySpanL="4"
                emptySpanM="4"
                emptySpanS="0"
                columnsXL="1"
                columnsL="1"
                columnsM="1"
                singleContainerFullSize="false"
            >
                <f:content>
                    <Label
                        id="_IDGenLabel1"
                        text="User ID"
                    />
                    <Input
                        id="name"
                        value="{}"
                        required="true"
                        maxLength="12"
                        liveChange="onLiveChange"
                    />
                    <Label
                        id="_IDGenLabel2"
                        text="Password"
                        required="true"
                    />
                    <Input
                        id="_IDGenInput1"
                        value=""
                        type="Password"
                        required="true"
                        maxLength="8"
                        liveChange="onLiveChange"
                    />
                    <Label
                        id="_IDGenLabrel2"
                        text="Are you a Babysitter"
                    />
                    <Label
                        id="_IDGetnLabrel2"
                        text=""
                    />
                    <Button
                        id="8r90"
                        press="onlogin"
                        text="LOGIN"
                        type="Accept"
                        width="50%"
                    />
                    <Label
                        id="_IDGeGnLabrrel2"
                        text="Are you a Babysitter"
                    />
                    <Button
                        id="8t90"
                        press="onBabysitterClick"
                        text="SignUp"
                        type="Transparent"
                        width="50%"
                    />
                    <Label
                        id="_IDGenLabrrel2"
                        text="Looking for a Babysitter"
                    />
                    <Button
                        id="890"
                        press="onparentclick"
                        text="SignUp"
                        type="Transparent"
                        width="50%"
                    />
                </f:content>
            </f:SimpleForm>
        </VBox>

        <Image
            id="_IDGenImage2"
            src="https://jaimelesstartups.b-cdn.net/wp-content/uploads/2018/07/Logo-Happysitters-png-1024x683.png.webp"
            height="90%"
            width="90%"
        />
    </Page>
</mvc:View>
