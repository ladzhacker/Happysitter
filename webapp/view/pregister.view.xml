<mvc:View
    controllerName="com.applexus.happysitter.controller.pregister"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <Toolbar id="_IDGenToolbar1">
            <Button
                id="_IDGenButton1"
                type="Back"
                press="onPress"
            />
            <ToolbarSpacer id="_IDGenToolbarSpacer1" />
            <Title
                id="_IDGenTitle1"
                text="Parent Registration"
                level="H2"
            />
            <ToolbarSpacer id="_IDGenToolbarSpacer2" />
        </Toolbar>
        <f:SimpleForm
            id="SimpleFormChange354"
            editable="true"
            layout="ResponsiveGridLayout"
            title="Basic Details"
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="3"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="4"
            emptySpanL="4"
            emptySpanM="4"
            emptySpanS="0"
            columnsXL="1"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false"
        >
            <f:content>
                <Label
                    id="_IDGenLabel7"
                    text="Name"
                    required="true"
                />
                <Input
                    id="name"
                    value="{}"
                    liveChange="onLiveChange"
                    maxLength="20"
                />

                <Label
                    id="_IDGenLabel9"
                    text="e-mail"
                    required="true"
                />
                <Input
                    id="ndame"
                    value="{}"
                    type="Email"
                    liveChange="onLiveChange"
                    maxLength="25"
                />
                <Label
                    id="_IDGenLabel8"
                    text="Password"
                    required="true"
                />
                <Input
                    id="ndagme"
                    value="{}"
                    type="Password"
                    liveChange="onLiveChange"
                    placeholder="Password must be 8 characters"
                    maxLength="8"
                />
                <Label
                    id="_IDGenLabel10"
                    text="Phonenumber"
                    required="true"
                />
                <Input
                    id="phone"
                    value="{}"
                    type="Number"
                    liveChange="onLiveChange"
                />
                <Label
                    id="_IDGenLabel11"
                    text="Gender"
                    required="true"
                />
                <RadioButtonGroup
                    id="r6bg3"
                    columns="5"
                    valueState="Error"
                    class="sapUiMediumMarginBottom"
                >
                    <RadioButton
                        id="RBY3-1"
                        text="M"
                    />
                    <RadioButton
                        id="RBG3-2"
                        text="F"
                    />
                </RadioButtonGroup>

                <Label
                    id="_IDGenLabel12"
                    text="Profession"
                />
                <Input
                    id="_IDGenInput5"
                    value=""
                />
                <Label
                    id="_IDGenLabel13"
                    text="Aadhaar No."
                    required="true"
                />
                <Input
                    id="_IHFDGenInput371"
                    value=""
                    type="Number"
                    maxLength="12"
                    liveChange="onLiveChange"
                />
            </f:content>
        </f:SimpleForm>

        <f:SimpleForm
            id="SimpleFormCjhange354"
            editable="true"
            layout="ResponsiveGridLayout"
            title="Address"
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="3"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="4"
            emptySpanL="4"
            emptySpanM="4"
            emptySpanS="0"
            columnsXL="1"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false"
        >
            <f:content>
                <Label
                    id="_IDGenLabel3"
                    text="House Name"
                    required="true"
                />
                <Input
                    id="hname"
                    value=""
                />
                <Label
                    id="_IDG"
                    text="Street"
                    required="true"
                />
                <Input
                    id="street"
                    value=""
                />
                <Label
                    id="_y"
                    text="Post Code"
                    required="true"
                />
                <Input
                    id="pcode"
                    value=""
                    liveChange="onLiveChange"
                    type="Number"
                />
                <Label
                    id="_yy"
                    text="City"
                    required="true"
                />
                <Input
                    id="city"
                    value=""
                />
            </f:content>
        </f:SimpleForm>
        <f:SimpleForm
            id="SimpleFiormCjhange354"
            editable="true"
            layout="ResponsiveGridLayout"
            title="Baby Details"
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="3"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="4"
            emptySpanL="4"
            emptySpanM="4"
            emptySpanS="0"
            columnsXL="1"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false"
        >
            <f:content>
                <Label
                    id="_IDGenLabel43"
                    text="Baby Name"
                    required="true"
                />
                <Input
                    id="bname"
                    value=""
                    liveChange="onLiveChange"
                />
                <Label
                    id="_IDGenL5yabel4"
                    text="Baby Gender"
                    required="true"
                />
                <RadioButtonGroup
                    id="bgender"
                    columns="5"
                    valueState="Error"
                    class="sapUiMediumMarginBottom"
                >
                    <RadioButton
                        id="RBY3yu"
                        text="M"
                    />
                    <RadioButton
                        id="RBGyui"
                        text="F"
                    />
                </RadioButtonGroup>

                <Label
                    id="bdob"
                    text="Baby DOB"
                    required="true"
                />
                <DatePicker
                    id="DP2"
                    change="handleChange"
                    class="sapUiSmallMarginBottom"
                />

                <Label
                    id="label6"
                    text="Medical allergies and Diet plan"
                />

                <TextArea
                    id="ta1"
                    width="100%"
                    placeholder="Enter Medical allergies and Diet plan for multiple babies as baby1
                    and baby2"
                />
                <Label
                    id="_IDGenLabel83"
                    text=""
                />
                <FlexBox
                    id="flx2"
                    height="200px"
                    alignItems="Start"
                    justifyContent="Center"
                >
                    <Button
                        id="btn2"
                        type="Accept"
                        text="Register"
                        press="oncreate"
                    />
                </FlexBox>
            </f:content>
        </f:SimpleForm>
    </Page>
</mvc:View>
